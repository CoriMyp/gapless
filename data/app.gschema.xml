<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="g4music">
  <schema id="@app_id@" path="/com/github/neithern/g4music/">
    <key name="tracker-mode" type="b">
      <default>false</default>
      <summary>Load music from tracker service</summary>
    </key>
    <key name="music-dir" type="s">
      <default>''</default>
      <summary>Load music from local folder</summary>
    </key>
    <key name="pipewire-sink" type="b">
      <default>false</default>
      <summary>Prefer pipewire audio sink</summary>
    </key>
    <key name="show-peak" type="b">
      <default>true</default>
      <summary>Show audio peak when playing</summary>
    </key>
    <key name="played-uri" type="s">
      <default>''</default>
      <summary>Last played uri when quit</summary>
    </key>
  </schema>
</schemalist>
